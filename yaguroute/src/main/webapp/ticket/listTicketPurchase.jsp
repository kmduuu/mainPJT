<head>
	<meta charset="UTF-8">
	<link href="https://fonts.googleapis.com/css?family=Montserrat%7COpen+Sans:700,400%7CRaleway:400,800,900" rel="stylesheet" />
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <link href="/css/style.min.css" rel="stylesheet" type="text/css" />
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
    <script type="text/javascript">
    
    
    $(function(){
		$( ".getTicketPurchaseDetail" ).on("click" , function() {
			var tranNo = $(this).siblings("input[name='tranNo']").val();
			self.location = "/ticket/getTicketPurchaseDetail?tranNo="+tranNo;
		});
    })
   
    </script>
</head>
<body>
		<table class="table table-hover table-striped" >
        <thead>
          <tr>
            <th align="center">예매날짜</th>
            <th align="left">결제번호</th>
            <th align="left">매치</th>
            <th align="left">경기일시</th>
            <th align="left">취소가능기간</th>
          </tr>
        </thead>
		  <tbody>
		  		<c:set var="size" value="${transaction.size()}"/>
				<c:forEach var="i" begin="0" end="${size -1}" step="1">
				<tr>
				  <td align="center">${transaction[i].tranDate}</td>
				  <td align="left">${transaction[i].tranNo}</td>
				  <td align="left">${game[i].homeTeam.teamFullName} vs ${game[i].awayTeam.teamFullName}</td>
				  <td align="left">${game[i].gameDate} ${game[i].gameTime}</td>
				  <td align="left">${game[i].gameDate}의 전날 24:00까지</td>
				  <td align="left">
				  		<input type="hidden" name="tranNo" value="${transaction[i].tranNo}">
				  		<button type="button" class="getTicketPurchaseDetail">상세보기</button>
				  </td>
				</tr>
	        	</c:forEach>
        </tbody>
      </table>
      
</body>
